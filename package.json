{
  "name": "aubinio",
  "version": "0.3.0",
  "scripts": {
    "ng": "nx",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck --ssl",
    "start:dev": "npm run start -- --configuration dev",
    "start:prod": "npm run start -- --configuration production",
    "build": "ng build",
    "build:dev": "npm run build -- --configuration dev",
    "build:prod": "npm run build -- --configuration production",
    "deploy:dev": "firebase deploy --non-interactive -P dev-aubinio --only hosting:dev-aubinio --token $FIREBASE_TOKEN",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --config ./.prettierrc --check \"src/{app,environments}/**/*{.ts,.html}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments}/**/*{.ts,.html}\"",
    "e2e": "ng e2e",
    "postinstall": "node ./decorate-angular-cli.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/fire": "^6.0.4",
    "@angular/forms": "~11.0.0",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@angular/service-worker": "~11.0.0",
    "@tonejs/midi": "^2.0.25",
    "firebase": "^7.0.0 || ^8.0.0",
    "pixi.js": "^5.3.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "uuid": "^8.3.1",
    "webmidi": "^2.5.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1002.0",
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular-eslint/builder": "0.8.0-beta.0",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.0",
    "@angular-eslint/schematics": "^0.8.0-beta.0",
    "@angular-eslint/template-parser": "0.8.0-beta.0",
    "@angular/cli": "~11.0.1",
    "@angular/compiler-cli": "~11.0.0",
    "@nrwl/workspace": "^10.3.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.4",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-unused-imports": "^1.0.1",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "open": "^7.3.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "rxjs-debug": "^1.3.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.5"
  }
}
